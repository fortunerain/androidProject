<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.sample.naverapi"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />
   	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    
    <application android:allowBackup="true" android:icon="@drawable/icon" android:label="@string/app_name">
       
         <activity android:name="com.sample.naverapi.MainActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>            
        </activity>
        
         <activity android:name="com.sample.naverapi.PriceListActivity"
                  android:label="@string/app_name">           
        </activity>
        
         <activity android:name="com.sample.naverapi.DetailActivity"
                  android:label="@string/app_name">           
        </activity>

    </application>
   

</manifest> 